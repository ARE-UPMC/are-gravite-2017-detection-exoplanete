    "def position_cdm(m,a,M,b):\n",
    "    ax,ay=a\n",
    "    bx,by=b\n",
    "    cdmx=(m*ax+M*bx)/(m+M)\n",
    "    cdmy=(m*ay+M*bx)/(m+M)\n",
    "    return cdmx,cdmy\n",
    "position_cdm(1,(0,3),2,(0,0))"
